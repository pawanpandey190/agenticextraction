# Education Credential Agent Configuration
# Copy this file to .env and fill in your values

# REQUIRED: OpenAI API key for vision analysis
EA_OPENAI_API_KEY=your_openai_api_key_here

# LLM Model Configuration
EA_LLM_MODEL=gpt-4o
EA_LLM_MAX_TOKENS=4096
EA_LLM_TEMPERATURE=0.0

# Academic level detection defaults
EA_DEFAULT_BACHELOR_SEMESTERS=8

# Processing limits
EA_MAX_FILE_SIZE_MB=50
EA_MAX_PDF_PAGES=100

# Logging
EA_LOG_LEVEL=INFO
