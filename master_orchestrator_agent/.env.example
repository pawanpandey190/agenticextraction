# Master Orchestrator Agent Configuration
# Copy this file to .env and fill in your values

# Required: OpenAI API key for document classification
MO_OPENAI_API_KEY=your-api-key-here

# Optional: Model selection (default: gpt-4o)
MO_MODEL_NAME=gpt-4o

# Optional: Classification settings
MO_CLASSIFICATION_STRATEGY=hybrid  # Options: hybrid, llm_only, filename_only
MO_NAME_MATCH_THRESHOLD=0.85  # Fuzzy matching threshold (0.0-1.0)

# Optional: Financial threshold in EUR (default: 15000)
MO_FINANCIAL_THRESHOLD_EUR=15000

# Optional: Output settings
MO_OUTPUT_FORMAT=both  # Options: json, excel, both
MO_LOG_LEVEL=INFO

# Optional: File size limits (in bytes)
MO_MAX_FILE_SIZE_BYTES=52428800  # 50MB default

# Sub-agent API keys (if different from master)
# If not set, MO_OPENAI_API_KEY will be used for all agents
# FA_OPENAI_API_KEY=your-financial-agent-api-key
# EA_OPENAI_API_KEY=your-education-agent-api-key
# PA_OPENAI_API_KEY=your-passport-agent-api-key
